{
	"name": "feedback-management-bot",
	"version": "0.0.1",
	"description": "A discord bot to streamline player feedback collection from multiple social media platforms",
	"author": "https://github.com/Jojoseis",
	"main": "dist/app.js",
	"type": "module",
	"scripts": {
		"build": "npm run lint && tsc",
		"lint": "biome check . --write && biome format . --write",
		"init": "node dist/init.js",
		"start": "node dist/app.js",
		"dev-init": "nodemon init.ts",
		"dev-start": "nodemon app.ts"
	},
	"dependencies": {
		"discord-interactions": "^4.4.0",
		"discord.js": "^14.25.1",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"node-cron": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/express": "^5.0.6",
		"@types/node": "^25.0.3",
		"nodemon": "^3.1.11",
		"typescript": "^5.9.3"
	}
}
